@model Models.ViewModel.OrganizationViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutOrganization.cshtml";
}
@Html.Partial("_NavigationBar", Model.IdOrganization)
<section id="content">
